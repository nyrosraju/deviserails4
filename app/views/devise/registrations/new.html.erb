<div class= "Sign_Form">
<h1>Sign Up</h1>



<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>

    <p>
      Email:</br>  <%= f.email_field :email, :autofocus => true %>
    </p>
<!--    <p>
      Location:</br> <%#= f.text_field :location %>
    </p>-->

    <p>      
      Password:</br> <%= f.password_field :password %>
    </p>

    <p>     
      Password Confirmation:</br> <%= f.password_field :password_confirmation %>
    </p>
  <div class="actions">
    <%= f.submit(:class=>"btn btn-success") %>
  </div>
  

	<ul class="Signup_Errors">
		
			<li>*   <%= devise_error_messages! %> </li>
	 
	</ul>

<% end %>
<%= render "devise/shared/links" %>

</div>